{
  "name": "pagewalker",
  "version": "0.6.11",
  "main": "index.js",
  "bin": [
    "bin/pagewalker",
    "bin/init-pagewalker-project"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xketanaka/pagewalker.git"
  },
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "debug": "DEBUG_LOG=query,app electron .",
    "esdoc": "esdoc"
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-node"
      }
    ]
  },
  "files": [
    "lib",
    "bin"
  ],
  "dependencies": {
    "bluebird": "^3.7.0",
    "log4js": "^6.4.0",
    "mocha": "^10.3.0",
    "pixelmatch": "^7.1.0",
    "ssim.js": "^3.5.0",
    "pngjs": "^7.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-node": "*",
    "esdoc-standard-plugin": "*"
  },
  "optionalDependencies": {
    "electron": "^22.3.25",
    "puppeteer": "^19.0"
  }
}
