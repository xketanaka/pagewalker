{
  "name": "pagewalker",
  "version": "0.3.13",
  "main": "index.js",
  "bin": "bin/pagewalker",
  "repository": {
    "type": "git",
    "url": "https://github.com/xketanaka/pagewalker.git"
  },
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "debug": "DEBUG_LOG=query,app electron .",
    "esdoc": "esdoc"
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-node"
      }
    ]
  },
  "dependencies": {
    "mocha": "*",
    "bluebird": "*",
    "uuid": "*",
    "pixelmatch": "xketanaka/pixelmatch"
  },
  "devDependencies": {
    "esdoc": "*",
    "esdoc-node": "*",
    "esdoc-standard-plugin": "*"
  },
  "optionalDependencies": {
    "electron": "^7.2.4",
    "puppeteer": "^1.0.0"
  }
}
